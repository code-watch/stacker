    "vuex": "^3.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-eslint": "^10.0.1",
    "electron": "^3.0.9",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.9.0",
    "eslint-plugin-vue": "^5.0.0-beta.4",
    "vue-template-compiler": "^2.5.17"
