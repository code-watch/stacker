{
  "name": "stacker",
  "description": "Multi IPC protocol GUI testing tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=DEV node_modules/.bin/vue-cli-service serve & sleep 5 && NODE_ENV=DEV electron electron.js",
