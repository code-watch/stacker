name: recursion
onInit:
    store:
        counter: 0
requisitions:
-   name: second.0
    iterations: 3
    requisitions:
    -   name: second.1
        iterations: 3
        requisitions:
        -   name: second.2
            iterations: 3
            requisitions:
            -   name: second.3
                iterations: 3
                onInit:
                    script: ++store.counter
onFinish:
    assertions:
    -   expect: store.counter
        toBeEqualTo: 3*3*3*3
